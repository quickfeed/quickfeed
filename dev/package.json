    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1",
    "selenium-webdriver": "^4.2.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  }
