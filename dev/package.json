  "devDependencies": {
    "@types/enzyme": "^3.10.11",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "@types/google-protobuf": "^3.15.3",
    "@types/jest": "^27.4.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/selenium-webdriver": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
