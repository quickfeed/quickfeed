{
  "name": "quickfeed",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "@bufbuild/connect-web": "^0.1.0",
    "@bufbuild/protobuf": "^0.0.10",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "google-protobuf": "^3.15.0-rc.1",
    "grpc-web": "^1.3.1",
    "overmind": "^28.0.1",
    "overmind-react": "^29.0.1",
    "react": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.3.0"
  },
  "scripts": {
    "tsc": "tsc",
    "test": "jest --testPathIgnorePatterns=e2e",
    "test:selenium": "jest --testPathPattern=e2e",
    "build": "webpack",
    "build:watch": "webpack --watch --mode development",
    "build:dev": "webpack --mode development"
  },
  "devDependencies": {
    "@bufbuild/protoc-gen-connect-web": "^0.1.0",
    "@bufbuild/protoc-gen-es": "^0.0.10",
    "@testing-library/react": "^12.1.5",
    "@types/google-protobuf": "^3.15.3",
    "@types/jest": "^27.4.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/selenium-webdriver": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^7.22.0",
    "eslint-plugin-react": "^7.24.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "process": "0.11.10",
    "react-dom": "^17.0.2",
    "selenium-webdriver": "^4.2.0",
    "source-map-loader": "^2.0.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^8.0.11",
    "typescript": "^4.6.2",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-middleware": "^5.3.3",
    "webpack-dev-server": "^4.8.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)",
      "!**/__tests__/TestHelpers.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ]
  }
}
